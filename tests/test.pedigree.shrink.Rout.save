R> ## use sample.ped from the package
R> data(sample.ped)
R> 
R> pedAll <- pedigree(sample.ped$id, sample.ped$father, sample.ped$mother, 
+        sample.ped$sex,
+        affected=cbind(sample.ped$affected, sample.ped$avail), 
+        famid=sample.ped$ped)
R> 
R> 
R> ped1 <- pedAll['1']
R> 
R> ped2 <- pedAll['2']
R> 
R> ped2$sex[c(13,12)] <- c("unknown", "terminated")
R> 
R> 
R> ## set 2nd col of affected to NA
R> ped2$affected[c(7,9),2] <- NA
R> 
R> 
